{
  "name": "bare-pack",
  "version": "1.0.0",
  "description": "Bundle packing for Bare",
  "exports": {
    ".": "./index.js",
    "./package": "./package.json"
  },
  "bin": {
    "bare-pack": "bin.js"
  },
  "files": [
    "index.js",
    "fs.js",
    "bin.js"
  ],
  "imports": {
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    },
    "url": {
      "bare": "bare-url",
      "default": "url"
    }
  },
  "scripts": {
    "test": "standard && bare test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/bare-pack.git"
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/bare-pack/issues"
  },
  "homepage": "https://github.com/holepunchto/bare-pack#readme",
  "dependencies": {
    "bare-bundle": "^1.8.0",
    "bare-fs": "^3.1.1",
    "bare-module-traverse": "^1.2.3",
    "bare-path": "^3.0.0",
    "bare-url": "^2.0.5",
    "paparam": "^1.5.0",
    "promaphore": "^1.0.0"
  },
  "devDependencies": {
    "brittle": "^3.1.1",
    "standard": "^17.0.0"
  }
}
